@prefix brick: <https://brickschema.org/schema/Brick#> .
@prefix hhws: <https://hhws.example.org#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rec: <https://w3id.org/rec#> .
@prefix ref: <https://brickschema.org/schema/Brick/ref#> .
@prefix unit: <http://qudt.org/vocab/unit/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

brick:Firing_Rate_Sensor a owl:Class ;
    rdfs:label "Firing Rate Sensor" ;
    rdfs:comment "A sensor that measures the firing rate of a boiler or similar combustion equipment, typically expressed as a percentage of maximum capacity." ;
    rdfs:subClassOf brick:Point .

<https://hhws.example.org> a owl:Ontology ;
    owl:imports <https://brickschema.org/schema/1.3/Brick#> .

hhws:building105 a rec:Building ;
    rdfs:label "Building 105" ;
    hhws:belongsToOrganization "H" ;
    hhws:hasArea "46000.0"^^xsd:float ;
    hhws:hasBuildingType "Office" ;
    hhws:hasClimateZone "3B" ;
    hhws:hasConstructionDecade 1980 ;
    hhws:hasConstructionYear 1980 ;
    hhws:hasDesignSupplyTemperature "87.77777778"^^xsd:float ;
    hhws:hasHeatingDesignTemperature "4.777777778"^^xsd:float ;
    hhws:hasHighLevelBuildingType "Office" ;
    hhws:hasHighLevelSystemType "Boiler" ;
    hhws:hasSystemType "Non-condensing" ;
    rec:isLocationOf hhws:building105.hws,
        hhws:building105.weather_station .

hhws:building105.boiler1 a brick:Noncondensing_Natural_Gas_Boiler ;
    rdfs:label "Non-condensing Natural Gas Boiler 1 - Building 105" ;
    brick:feeds hhws:building105.primary_pump1 ;
    hhws:hasManufacturer "Laars" ;
    hhws:hasMinimumTurndown "0.25"^^xsd:float ;
    hhws:hasModel "Pennant PNCH 2000" ;
    hhws:hasNominalEfficiency "0.8505"^^xsd:float ;
    hhws:hasNominalInputPower "586142.14"^^xsd:float ;
    hhws:hasNominalOutputPower "498513.8901"^^xsd:float .

hhws:building105.boiler2 a brick:Noncondensing_Natural_Gas_Boiler ;
    rdfs:label "Non-condensing Natural Gas Boiler 2 - Building 105" ;
    brick:feeds hhws:building105.primary_pump1 ;
    hhws:hasManufacturer "Laars" ;
    hhws:hasMinimumTurndown "0.25"^^xsd:float ;
    hhws:hasModel "Pennant PNCH 2000" ;
    hhws:hasNominalEfficiency "0.8505"^^xsd:float ;
    hhws:hasNominalInputPower "586142.14"^^xsd:float ;
    hhws:hasNominalOutputPower "498513.8901"^^xsd:float .

hhws:building105.boiler3 a brick:Noncondensing_Natural_Gas_Boiler ;
    rdfs:label "Non-condensing Natural Gas Boiler 3 - Building 105" ;
    brick:feeds hhws:building105.primary_pump1 ;
    hhws:hasManufacturer "Laars" ;
    hhws:hasMinimumTurndown "0.25"^^xsd:float ;
    hhws:hasModel "Pennant PNCH 2000" ;
    hhws:hasNominalEfficiency "0.8505"^^xsd:float ;
    hhws:hasNominalInputPower "586142.14"^^xsd:float ;
    hhws:hasNominalOutputPower "498513.8901"^^xsd:float .

hhws:building105.hws a brick:Hot_Water_System ;
    rdfs:label "Hot Water System - Building 105" ;
    brick:hasPart hhws:building105.hws.oper,
        hhws:building105.hws.primary_loop,
        hhws:building105.hws.secondary_loop ;
    hhws:hasBoilerCount 3 .

hhws:building105.hws.oper a brick:Enable_Status ;
    rdfs:label "Hot water system operation status (0=off, 1=on, fractional values indicate the system was estimated to be operating for part of the hour) - Building 105" ;
    brick:hasUnit unit:UNITLESS ;
    ref:hasExternalReference [ a ref:TimeseriesReference ;
            ref:hasTimeseriesId "oper" ;
            ref:storedAt "105hhw_system_data.csv" ] .

hhws:building105.hws.primary_loop a brick:Hot_Water_Loop ;
    rdfs:label "Primary Hot Water Loop - Building 105" ;
    brick:feeds hhws:building105.hws.secondary_loop ;
    brick:hasPart hhws:building105.boiler1,
        hhws:building105.boiler2,
        hhws:building105.boiler3,
        hhws:building105.primary_pump1 .

hhws:building105.hws.secondary_loop.flow a brick:Flow_Sensor ;
    rdfs:label "Flow rate of water entering the building - Building 105" ;
    brick:hasUnit unit:L-PER-SEC ;
    ref:hasExternalReference [ a ref:TimeseriesReference ;
            ref:hasTimeseriesId "flow" ;
            ref:storedAt "105hhw_system_data.csv" ] .

hhws:building105.hws.secondary_loop.hw a brick:Thermal_Power_Sensor ;
    rdfs:label "Heating power (or load) being supplied to the building - Building 105" ;
    brick:hasUnit unit:W ;
    ref:hasExternalReference [ a ref:TimeseriesReference ;
            ref:hasTimeseriesId "hw" ;
            ref:storedAt "105hhw_system_data.csv" ] .

hhws:building105.hws.secondary_loop.ret a brick:Entering_Hot_Water_Temperature_Sensor ;
    rdfs:label "Return water temperature leaving the building - Building 105" ;
    brick:hasUnit unit:DEG_C ;
    ref:hasExternalReference [ a ref:TimeseriesReference ;
            ref:hasTimeseriesId "ret" ;
            ref:storedAt "105hhw_system_data.csv" ] .

hhws:building105.hws.secondary_loop.sup a brick:Leaving_Hot_Water_Temperature_Sensor ;
    rdfs:label "Supply water temperature entering the building - Building 105" ;
    brick:hasUnit unit:DEG_C ;
    ref:hasExternalReference [ a ref:TimeseriesReference ;
            ref:hasTimeseriesId "sup" ;
            ref:storedAt "105hhw_system_data.csv" ] .

hhws:building105.hws.secondary_loop.sup_stpt a brick:Hot_Water_Temperature_Setpoint ;
    rdfs:label "Setpoint for supply water temperature entering the building - Building 105" ;
    brick:hasUnit unit:DEG_C ;
    ref:hasExternalReference [ a ref:TimeseriesReference ;
            ref:hasTimeseriesId "sup_stpt" ;
            ref:storedAt "105hhw_system_data.csv" ] .

hhws:building105.secondary_pump1 a brick:Pump ;
    rdfs:label "Secondary Circulation Pump 1 - Building 105" .

hhws:building105.weather_station a brick:Weather_Station ;
    rdfs:label "Weather Station - Building 105" ;
    brick:hasPoint hhws:building105.weather_station.t_out .

hhws:building105.weather_station.t_out a brick:Outside_Air_Temperature_Sensor ;
    rdfs:label "Outdoor drybulb temperature - Building 105" ;
    brick:hasUnit unit:DEG_C ;
    ref:hasExternalReference [ a ref:TimeseriesReference ;
            ref:hasTimeseriesId "t_out" ;
            ref:storedAt "105hhw_system_data.csv" ] .

hhws:building105.hws.secondary_loop a brick:Hot_Water_Loop ;
    rdfs:label "Secondary Hot Water Loop - Building 105" ;
    brick:hasPart hhws:building105.hws.secondary_loop.flow,
        hhws:building105.hws.secondary_loop.hw,
        hhws:building105.hws.secondary_loop.ret,
        hhws:building105.hws.secondary_loop.sup,
        hhws:building105.hws.secondary_loop.sup_stpt,
        hhws:building105.secondary_pump1 .

hhws:building105.primary_pump1 a brick:Pump ;
    rdfs:label "Primary Circulation Pump 1 - Building 105" .

